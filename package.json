{
  "name": "neon-auth-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "prettier -c . --log-level warn",
    "format:fix": "prettier -w . --log-level warn",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint": "npm-run-all format lint:ts lint:eslint",
    "lint:fix": "npm-run-all format:fix lint:ts lint:eslint:fix",
    "prepreview": "npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@stackframe/react": "^2.8.3",
    "@tailwindcss/vite": "^4.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.5.0",
    "tailwindcss": "^4.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
